{
  "name": "notesify",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "node ./dist/server/server.js",
    "preview": "vite preview",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "run:desktop": "tauri dev",
    "run:ios": "tauri ios dev 'iPad Air 13-inch (M2)'",
    "run:android": "tauri android dev",
    "build:ios": "tauri ios build --export-method app-store-connect",
    "build:android": "tauri android build --apk",
    "build:android-store": "tauri android build --aab",
    "build:mac": "tauri build --bundles dmg",
    "build:mac-store": "tauri build --no-bundle && tauri bundle --bundles app --target universal-apple-darwin --config src-tauri/tauri.appstore.conf.json",
    "build:windows": "tauri build --runner cargo-xwin --target x86_64-pc-windows-msvc",
    "db:generate": "drizzle-kit generate && tsx ./scripts/compile-migrations.ts",
    "db:push": "drizzle-kit push && tsx ./scripts/compile-migrations.ts",
    "db:studio": "drizzle-kit studio",
    "check": "biome check --write",
    "prepare": "husky"
  },
